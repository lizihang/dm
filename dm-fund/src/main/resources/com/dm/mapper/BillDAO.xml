<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dm.fund.dao.BillDAO">

	<select id="queryList" resultType="Bill" parameterType="BillQueryParams">
		select id,usercode from dm_bill
		<where>
			<if test="userCode != null">
				and user.usercode = #{userCode}
			</if>
			<if test="tradeDirection != null">
				and user.tradedirection = #{tradeDirection}
			</if>
		</where>
	</select>

</mapper>